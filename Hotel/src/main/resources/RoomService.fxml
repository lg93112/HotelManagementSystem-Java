<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="rootPane" prefHeight="337.0" prefWidth="446.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RoomService.RoomServiceController">
   <children>
      <VBox prefHeight="379.0" prefWidth="461.0">
         <children>
            <JFXTextField fx:id="room_service_name" labelFloat="true" promptText="Guest Name">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="60.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="room_service_num" labelFloat="true" layoutX="20.0" layoutY="30.0" promptText="Room Number">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="room_service_req" labelFloat="true" layoutX="10.0" layoutY="66.0" promptText="Requirements">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <HBox prefHeight="208.0" prefWidth="461.0">
               <children>
                  <JFXButton fx:id="room_service_save" onAction="#addRoomService" prefHeight="118.0" prefWidth="237.0" stylesheets="@checkin.css" text="Save" />
                  <JFXButton fx:id="room_service_cancel" layoutX="10.0" layoutY="10.0" onAction="#cancelRoomService" prefHeight="118.0" prefWidth="248.0" stylesheets="@checkin.css" text="Cancel" />
               </children>
               <VBox.margin>
                  <Insets top="60.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
